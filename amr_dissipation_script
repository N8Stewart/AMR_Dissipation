#PBS -l nodes=1:ppn=1
#PBS -l mem=4gb
#PBS -j oe
# Set up the FLUENT environment

# Make the amr_dissipation program
cd $HOME/amr_dissipation
make opt3

# Move a.out into compute node
cp a.out $TMPDIR/.
cp testgrids/testgrid_400_12206 $TMPDIR/testgrid
cd $TMPDIR

# Execute a.out
./a.out 0.1 0.1 < testgrid > results
cp results $HOME/amr_dissipation/results/.

