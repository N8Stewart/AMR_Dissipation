#PBS -N N8_AMR_Diss
#PBS -l walltime=0:02:00
#PBS -l nodes=1:ppn=12
#PBS -l mem=1gb
#PBS -j oe
# Set up the FLUENT environment

# Make the amr_dissipation program
cd $PBS_O_WORKDIR
make opt3

# Move a.out into compute node
cp a.out $TMPDIR/.
cp testgrids/testgrid_400_12206 $TMPDIR/testgrid
cd $TMPDIR

# Execute a.out
time ./a.out 0.1 0.1 < testgrid > results
cp results $PBS_O_WORKDIR/results/.

